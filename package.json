{
  "name": "chunklands",
  "version": "1.0.0",
  "description": "3D Game in NodeJS/OpenGL",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon --watch src --ext hxx,cxx,inl --exec \"make\"",
    "dev:build": "make",
    "test": "tap src/**/*.test.js",
    "pretest": "make test",
    "test:watch": "nodemon --watch src --ext hxx,cxx,inl --exec \"npm test\"",
    "play": "node src/app.js",
    "preplay": "cross-env NODE_ENV=production make"
  },
  "keywords": [
    "game",
    "c++",
    "opengl",
    "nodejs",
    "napi",
    "node-addon-api",
    "boost"
  ],
  "license": "MIT",
  "dependencies": {
    "image-size": "^0.8.3",
    "jimp": "^0.9.3"
  },
  "devDependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^6.1.0",
    "concurrently": "^4.1.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "execa": "^4.0.2",
    "glob": "^7.1.6",
    "node-addon-api": "^2.0.0",
    "nodemon": "^2.0.3",
    "sinon": "^9.0.2",
    "tap": "^14.10.7"
  }
}
